{% extends "base.html.twig" %}

{% block body %}
<h3>Créer un Pokémon</h3>
{{form_start(form, {"attr": {"novalidate": "novalidate"}})}}
    {{ form_row(form.number) }}
    {{ form_row(form.name) }}
    {{ form_row(form.area) }}
    {{ form_row(form.image) }}
    <input type="submit" value="Créer" class="btn btn-primary">
{{form_end(form)}}
{% endblock %}